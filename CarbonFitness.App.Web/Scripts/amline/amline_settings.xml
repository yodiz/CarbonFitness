<settings>
	<connect>1</connect>
	<add_time_stamp>1</add_time_stamp>
	<digits_after_decimal>2</digits_after_decimal>
	<legend>
		<values>
			<enabled>1</enabled>
			<width>44</width>
			<align>left</align>
			<text>: ${value}</text>
		</values>
	</legend>
	<graphs>
		<graph gid="1">
			<title>Nominal</title>
			<color>009933</color>
			<line_width>2</line_width>
			<balloon_text>${value}</balloon_text>
			<bullet>round_outline</bullet>
		</graph>
		<graph gid="2">
			<title>Inflation adjusted</title>
			<color>999999</color>
			<selected>0</selected>
			<balloon_text>{value} kg</balloon_text>
		</graph>
	</graphs>
	<labels>
		<label lid="0">
			<y>20</y>
			<width>520</width>
			<align>center</align>
		</label>
	</labels>
	<guides>
		<guide gid="1"/>
	</guides>
</settings>